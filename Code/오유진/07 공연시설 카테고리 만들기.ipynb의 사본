 Load and filter data for each year\n","columns_to_include = ['공연시설코드', '예매/취소구분']\n","\n","# Load and filter data for 2019\n","YM19 = pd.read_csv('/content/drive/MyDrive/KOPIS DATA/Data/csv/all/YM/2019YM.csv', usecols=columns_to_include)\n","YM19_filtered = YM19[YM19['예매/취소구분'] == 1]\n","counted_data19 = YM19_filtered.groupby('공연시설코드').size().reset_index(name='총예매수19')\n","del YM19,YM19_filtered\n","# Load and filter data for 2020\n","YM20 = pd.read_csv('/content/drive/MyDrive/KOPIS DATA/Data/csv/all/YM/2020YM.csv', usecols=columns_to_include)\n","YM20_filtered = YM20[YM20['예매/취소구분'] == 1]\n","counted_data20 = YM20_filtered.groupby('공연시설코드').size().reset_index(name='총예매수20')\n","del YM20,YM20_filtered\n","\n","# Load and filter data for 2021\n","YM21 = pd.read_csv('/content/drive/MyDrive/KOPIS DATA/Data/csv/all/YM/2021YM.csv', usecols=columns_to_include)\n","YM21_filtered = YM21[YM21['예매/취소구분'] == 1]\n","counted_data21 = YM21_filtered.groupby('공연시설코드').size().reset_index(name='총예매수21')\n","del YM21,YM21_filtered\n","\n","# Load and filter data for 2022 (part 1)\n","YM22_1 = pd.read_csv('/content/drive/MyDrive/KOPIS DATA/Data/csv/all/YM/2022YM(1).csv', usecols=columns_to_include)\n","YM22_1_filtered = YM22_1[YM22_1['예매/취소구분'] == 1]\n","counted_data22_1 = YM22_1_filtered.groupby('공연시설코드').size().reset_index(name='총예매수22_1')\n","del YM22_1,YM22_1_filtered\n","\n","# Load and filter data for 2022 (part 2)\n","YM22_2 = pd.read_csv('/content/drive/MyDrive/KOPIS DATA/Data/csv/all/YM/2022YM(2).csv', usecols=columns_to_include)\n","YM22_2_filtered = YM22_2[YM22_2['예매/취소구분'] == 1]\n","counted_data22_2 = YM22_2_filtered.groupby('공연시설코드').size().reset_index(name='총예매수22_2')\n","del YM22_2,YM22_2_filtered\n","\n","# Merge the dataframes on '공연시설코드'\n","counted_data = counted_data19.merge(counted_data20, on='공연시설코드', how='outer')\n","counted_data = counted_data.merge(counted_data21, on='공연시설코드', how='outer')\n","counted_data = counted_data.merge(counted_data22_1, on='공연시설코드', how='outer')\n","counted_data = counted_data.merge(counted_data22_2, on='공연시설코드', how='outer')\n","\n","# Fill missing values with 0\n","counted_data.fillna(0, inplace=True)\n","\n","counted_data.head()\n","\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":206},"id":"o81eXzQmxBuG","executionInfo":{"status":"ok","timestamp":1692369964821,"user_tz":-540,"elapsed":265887,"user":{"displayName":"Yujin Oh","userId":"08773599218574211134"}},"outputId":"dc12793e-7814-4e14-cdc0-688e85f4f8dd"},"execution_count":null,"outputs":[{"output_type":"execute_result","data":{"text/plain":["     공연시설코드    총예매수19    총예매수20    총예매수21  총예매수22_1  총예매수22_2\n","0  FC222005    4072.0    1421.0       0.0       0.0      99.0\n","1  FC222022     440.0       0.0       0.0    1047.0     428.0\n","2  FC222024     800.0       0.0       0.0       0.0       0.0\n","3  FC222028  240116.0  160111.0  137374.0  141182.0  104552.0\n","4  FC222029   25215.0       0.0       0.0       0.0   26375.0"],"text/html":["\n","  <div id=\"df-da280198-2f6f-437f-bb06-cceecf1288ae\" class=\"colab-df-container\">\n","    <div>\n","<style scoped>\n","    .dataframe tbody tr th:only-of-type {\n","        vertical-align: middle;\n","    }\n","\n","    .dataframe tbody tr th {\n","        vertical-align: top;\n","    }\n","\n","    .dataframe thead th {\n","        text-align: right;\n","    }\n","</style>\n","<table border=\"1\" class=\"dataframe\">\n","  <thead>\n","    <tr style=\"text-align: right;\">\n","      <th></th>\n","      <th>공연시설코드</th>\n","      <th>총예매수19</th>\n","      <th>총예매수20</th>\n","      <th>총예매수21</th>\n","      <th>총예매수22_1</th>\n","      <th>총예매수22_2</th>\n","    </tr>\n","  </thead>\n","  <tbody>\n","    <tr>\n","      <th>0</th>\n","      <td>FC222005</td>\n","      <td>4072.0</td>\n","      <td>1421.0</td>\n","      <td>0.0</td>\n","      <td>0.0</td>\n","      <td>99.0</td>\n","    </tr>\n","    <tr>\n","      <th>1</th>\n","      <td>FC222022</td>\n","      <td>440.0</td>\n