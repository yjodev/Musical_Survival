d.groupby('공연시설코드').size().reset_index(name='총예매수22_1')\n","del YM22_1,YM22_1_filtered\n","\n","# Load and filter data for 2022 (part 2)\n","YM22_2 = pd.read_csv('/content/drive/MyDrive/KOPIS DATA/Data/csv/all/YM/2022YM(2).csv', usecols=columns_to_include)\n","YM22_2_filtered = YM22_2[YM22_2['예매/취소구분'] == 1]\n","counted_data22_2 = YM22_2_filtered.groupby('공연시설코드').size().reset_index(name='총예매수22_2')\n","del YM22_2,YM22_2_filtered\n","\n","# Merge the dataframes on '공연시설코드'\n","\n","counted_data = counted_data22_1.merge(counted_data22_2, on='공연시설코드', how='outer')\n","\n","# Fill missing values with 0\n","counted_data.fillna(0, inplace=True)\n","\n","counted_data.head()"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":206},"id":"eFwxqt1KUtj7","executionInfo":{"status":"ok","timestamp":1692951099467,"user_tz":-540,"elapsed":158274,"user":{"displayName":"Yujin Oh","userId":"08773599218574211134"}},"outputId":"7c5e7e83-86b1-490a-f03b-264eaa6e14b9"},"execution_count":5,"outputs":[{"output_type":"execute_result","data":{"text/plain":["     공연시설코드  총예매수22_1  총예매수22_2\n","0  FC222022    1047.0     428.0\n","1  FC222028  141182.0  104552.0\n","2  FC222032    4662.0    1281.0\n","3  FC222039     342.0    1173.0\n","4  FC222047     103.0     534.0"],"text/html":["\n","  <div id=\"df-4b156d13-1c40-4344-a836-ed6db59f3a01\" class=\"colab-df-container\">\n","    <div>\n","<style scoped>\n","    .dataframe tbody tr th:only-of-type {\n","        vertical-align: middle;\n","    }\n","\n","    .dataframe tbody tr th {\n","        vertical-align: top;\n","    }\n","\n","    .dataframe thead th {\n","        text-align: right;\n","    }\n","</style>\n","<table border=\"1\" class=\"dataframe\">\n","  <thead>\n","    <tr style=\"text-align: right;\">\n","      <th></th>\n","      <th>공연시설코드</th>\n","      <th>총예매수22_1</th>\n","      <th>총예매수22_2</th>\n","    </tr>\n","  </thead>\n","  <tbody>\n","    <tr>\n","      <th>0</th>\n","      <td>FC222022</td>\n","      <td>10