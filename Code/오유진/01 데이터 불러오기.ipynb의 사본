ent = document.querySelector('#df-703ee6b9-0b4a-4656-ad79-0527def90400');\n","          const dataTable =\n","            await google.colab.kernel.invokeFunction('convertToInteractive',\n","                                                     [key], {});\n","          if (!dataTable) return;\n","\n","          const docLinkHtml = 'Like what you see? Visit the ' +\n","            '<a target=\"_blank\" href=https://colab.research.google.com/notebooks/data_table.ipynb>data table notebook</a>'\n","            + ' to learn more about interactive tables.';\n","          element.innerHTML = '';\n","          dataTable['output_type'] = 'display_data';\n","          await google.colab.output.renderOutput(dataTable, element);\n","          const docLink = document.createElement('div');\n","          docLink.innerHTML = docLinkHtml;\n","          element.appendChild(docLink);\n","        }\n","      </script>\n","    </div>\n","  </div>\n"]},"metadata":{},"execution_count":71}]},{"cell_type":"markdown","source":["- 공연일시는 바뀌는것같고\n","- 나머지는 다 똑같은가?\n","- 특정 공연코드는 출연진까지 고정 ?\n"],"metadata":{"id":"JvWKKHLbcjWx"}},{"cell_type":"code","source":["columns_to_check = ['공연회차', '공연일시','공연시작일자', '공연종료일자', '소요시간', '장르명', '세부장르명', '공연지역명', '출연진내용', '제작진내용', '기획제작사명', '원작자명', '극작가명', '수상실적', '관람연령', '아동공연 여부', '축제 여부', '내한공연 여부', '오픈런 여부']\n","\n","# Step 2: Group the DataFrame by '공연코드' and check if all rows within each group have the same values in the specified columns\n","grouped = df.groupby('공연코드')\n","same_values = grouped[columns_to_check].nunique()\n","\n","# Step 3: Check if the count of unique values is 1 for all columns in each group\n","all_columns_have_same_values = (same_values == 1).all()\n","\n","# Now 'all_columns_have_same_values' will be True if all rows with the same '공연코드' have the same values in the specified columns, otherwise, it will be False.\n","print(all_columns_have_same_values)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"q2sZYFivcsne","e