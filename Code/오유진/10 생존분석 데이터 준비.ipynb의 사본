tion')['Count'].sum()\n","\n","\n","print(final_section_sums)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"10ZXHFuhsnw4","executionInfo":{"status":"ok","timestamp":1692850024044,"user_tz":-540,"elapsed":466,"user":{"displayName":"Yujin Oh","userId":"08773599218574211134"}},"outputId":"bea7e784-6a6c-4f9d-bb0c-82476de4b2ab"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["     판매기간   Count  Section\n","0       1  832429        0\n","7       2  479837        0\n","70      3   92499        0\n","80      4   66785        0\n","85      5   46220        0\n","..    ...     ...      ...\n","243   247       3        4\n","246   248       2        4\n","235   249       7        4\n","232   251       8        4\n","225   257      13        4\n","\n","[248 rows x 3 columns]\n","Section\n","0    5474631\n","1    5519574\n","2    5647773\n","3    5650523\n","4     386061\n","Name: Count, dtype: int64\n"]}]},{"cell_type":"code","source":["df.head(30)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":990},"id":"Y6iIM9cRtLT2","executionInfo":{"status":"ok","timestamp":1692850027298,"user_tz":-540,"elapsed":5,"user":{"displayName":"Yujin Oh","userId":"08773599218574211134"}},"outputId":"b02a30b7-a895-4f62-9494-a5878fdb0119"},"execution_count":null,"outputs":[{"output_type":"execute_result","data":{"text/plain":["    판매기간   Count  Section\n","0      1  832429        0\n","7      2  479837        0\n","70     3   92499        0\n","80     4   66785        0\n","85     5   46220        0\n","86     6   44231        0\n","81     7   64853        0\n","66     8  106318        0\n","74     9   86987        0\n","64    10  113078        0\n","68    11  105132        0\n","67    12  105185        0\n","65    13  112980        0\n","75    14   85958        0\n","62    15  131732        0\n","39    16  225954        0\n","42    17  214016        0\n","51    18  189713        0\n","53    19  184118        0\n","59    20